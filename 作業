#include <iostream>
#include <cmath>

using namespace std;







int main(){

	float v[7][7] = {{1,35,80,40},{2,19,60,32},{3,56,90,42},{4,70,98,88},{5,92,24,35}};

	

	//總分 

	for (int i = 0; i<6; i++){

		int temp;

		temp = v[i][1]+v[i][2]+v[i][3];

		v[i][4] = temp;

	}

	

	//學生平均 

	for (int i = 0; i<6; i++){

		v[i][5] = v[i][4]/3;

	}

	

	//各科平均 

	float sum;

	for (int i = 1; i<4; i++){

		sum = 0;

		for (int j = 0; j<5; j++){

			sum += v[j][i];

		}

		v[5][i] = sum/5;

	}

	

    //排名

    float r[5][2];

    for (int i = 0; i < 5; i++)

    {

        r[i][0] = v[i][5];
        r[i][1] = i;

    }

    

    for (int j = 0; j < 5; j++)

    {

        for (int i = 0; i < 5-j; i++)

        {

            if (r[i][0]<r[i+1][0])

            {

                float t1,t2;

                t1 = r[i][0];
                t2 = r[i][1];

                r[i][0] = r[i+1][0];
                r[i][1] = r[i+1][1];

                r[i+1][0] = t1;
                r[i+1][1] = t2;

            }

        }

    }
    
    int number;

    for(int i = 0; i<5; i++){
        number = r[i][1];
        v[number][6]=i+1;
    }
    
    //標準差
    float standsum;
    for(int i = 1; i<4; i++){
        standsum = 0;
        for(int j = 0; j<5; j++){
            standsum += v[j][i]*v[j][i];
        }
        standsum /= 5;
        standsum -= v[5][i]*v[5][i];
        standsum= sqrt(standsum);
        v[6][i] = standsum;
    }
    
    
    
    

	//列印

	for (int i = 0; i<7; i++){

		for (int j = 0; j<7; j++){

			cout<<v[i][j]<<"\t";

		}

		cout<<endl;

	}

}

    
