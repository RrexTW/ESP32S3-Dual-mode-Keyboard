#include <iostream>
using namespace std;



int main(){
	float v[7][6] = {{35,80,40},{19,60,32},{56,90,42},{70,98,88},{92,24,35}};
	
	//總分 
	for (int i = 0; i<6; i++){
		int temp;
		temp = v[i][0]+v[i][1]+v[i][2];
		v[i][3] = temp;
	}
	
	//學生平均 
	for (int i = 0; i<6; i++){
		v[i][4] = v[i][3]/3;
	}
	
	//名次 
	
	//各科平均 
	int sum;
	for (int i = 0; i<3; i++){
		sum = 0;
		for (int j = 0; j<5; i++){
			sum += v[i][j];
		}
		v[i][5] = sum;
	}
	
	
	
	
	for (int i = 0; i<7; i++){
		for (int j = 0; j<6; j++){
			cout<<v[i][j]<<"\t";
		}
		cout<<endl;
	}
	
}

